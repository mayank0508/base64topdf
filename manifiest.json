{
  "manifest_version": 3,
  "name": "Base64 to PDF Converter",
  "version": "1.0",
  "description": "Convert Base64 strings to PDF files.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["downloads"]
}
